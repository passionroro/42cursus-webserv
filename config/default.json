{
  "servers": [
    {
      "server_name": "www.example.com",
      "address": ["127.0.0.1", "localhost"],
      "port": 8082,
      "disabled_methods": [""],
      "auto_index": true,
      "error_pages": {
        "path": "home/www/error_pages/",
        "400": "400.html",
        "404": "404.html",
        "405": "405.html",
        "409": "409.html",
        "500": "500.html",
        "501": "501.html"
      },
      "cgi": [
        {
          "extension": ".php",
          "bin": "./cgi-bin/php-cgi"
        },
        {
          "extension": ".sh",
          "bin": "/bin/bash"
        }
      ],
      "locations": [
        {
          "path": "/",
          "index": "index.html",
          "client_max_body_size": "0",
      	  "root": "home/www"
        },
        {
          "path": "/site2",
          "index": "index.html",
          "client_max_body_size": "2000000",
          "root": "home/www"
        }
      ]
    }
  ]
}
