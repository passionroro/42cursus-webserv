{
  "servers": [
    {
      "server_name": "www.example.com",
      "address": ["127.0.0.1", "localhost"],
      "port": 8082,
      "client_max_body_size": 0,
      "disabled_methods": [""],
      "auto_index": true,
      "error_pages": {
        "path": "/error_pages",
        "400": "custom_400.html",
        "403": "custom_403.html",
        "404": "custom_404.html",
        "405": "custom_405.html",
        "410": "custom_410.html",
        "413": "custom_413.html",
        "500": "custom_500.html"
      },
      "locations": [
        {
          "path": "/",
          "index": "index.html",
          "client_max_body_size": "0",
      	  "root": "home/www"
        },
        {
          "path": "/cgi-bin",
		  "index": "index.php",
		  "bin": "cgi-hello",
      	  "root": "home/cgi",
          "client_max_body_size": "50000"
        },
        {
          "path": "/images",
          "root": "home/www",
          "client_max_body_size": "2000000"
        }
      ]
    }
  ]
}
