{
  "servers": [
    {
      "server_name": "www.example.com",
      "address": ["127.0.0.1", "localhost"],
      "port": 8082,
      "client_max_body_size": 0,
      "disabled_methods": [""],
      "auto_index": true,
      "error_pages": {
        "path": "/error_pages",
        "400": "custom_400.html",
        "403": "custom_403.html",
        "404": "custom_404.html",
        "405": "custom_405.html",
        "410": "custom_410.html",
        "413": "custom_413.html",
        "500": "custom_500.html"
      },
      "cgi": [
        {
          "extension": ".php",
          "bin": "/opt/homebrew/bin/php-cgi"
        },
        {
          "extension": ".sh",
          "bin": "/bin/bash"
        }
      ],
      "locations": [
        {
          "path": "/",
          "index": "index.html",
          "client_max_body_size": "0",
      	  "root": "home/www"
        },
        {
          "path": "/site2",
          "index": "index.html",
          "root": "home/www"
        },
        {
          "path": "/uploads",
          "index": "uploads",
          "client_max_body_size": "2000000",
          "root": "home/www"
        }
      ]
    }
  ]
}
